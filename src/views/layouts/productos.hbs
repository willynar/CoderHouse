{{> headers}}
{{> navegador}}
<br>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Titulo</th>
            <th scope="col">Precio</th>
            <th scope="col">Imagen</th>
        </tr>
    </thead>
    {{#if lista}}
    <tbody>
        {{#each lista}}
        <tr>
            <th scope="row">{{this.id}}</th>
            <td>{{this.title}}</td>
            <td>@{{this.price}}</td>
            <td><img src="{{this.thumbnail}}" alt="" style="width:30px ;"> </td>
        </tr>
        {{/each}}
    </tbody>
    {{else}}
    <tbody>
        <tr>
            <td colspan="4">Sin datos</td>
        </tr>
    </tbody>
    {{/if}}
</table>
{{> scripts}}